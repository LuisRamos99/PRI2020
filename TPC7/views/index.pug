extends layout

block content
  h1= title
  header.w3-container.w3-teal
    h3 Lista de Alunos
    if lista.length > 0 
      .w3-container
      table(class="w3-table-all")
        tr 
          th Número
          th Nome
          th Git
        each a in lista
        tr
          td= a.Número
          td= a.Nome
          td= a.Git

